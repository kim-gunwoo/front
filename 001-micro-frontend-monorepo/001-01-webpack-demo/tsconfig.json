{
  "compilerOptions": {
    "target": "ES5",
    "jsx": "react",
    "module": "commonjs",
    "moduleResolution": "node",
    "esModuleInterop": true,
    "declaration": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true,
  },
  "include": ["**/*.ts", "**/*.tsx", "src/pages/index.page copy.jsx"],
  "exclude": ["node_modules"],
}